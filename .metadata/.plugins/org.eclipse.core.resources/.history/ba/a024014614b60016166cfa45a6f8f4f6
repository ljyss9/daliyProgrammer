package test1;
import java.util.*;

public class Main{
	 public String removeKdigits(String num, int k) {
	        if(num.length() == k)
	        	return "0";
		 	StringBuffer stemp = new StringBuffer(num);
	        boolean flag = false;
	        while(k-->0){
	            int i;
	            flag = false;
	            for( i = 0;i < stemp.length() - 1;i++){
	                if(stemp.charAt(i) > stemp.charAt(i+1))
	                {
	                	flag = true;
	                    stemp.deleteCharAt(i);
	                    break;
	                }
	            }
	            if(i == stemp.length() - 1){
	                stemp.deleteCharAt(i);
	            }
	        }
	        int i = 0;
	        while(i < stemp.length() && stemp.charAt(i) == '0'){
	            i++;
	        }
	        if(i != 0)
	            stemp.delete(0,i);
	        if(stemp.length() !=0)
	        	return String.valueOf(stemp);
	        else
	        	return "0";
	    }
	    public static void main(String args[]){
	    	Main m = new Main();
	    	System.out.println(m.removeKdigits("1173", 2));
	    }
}