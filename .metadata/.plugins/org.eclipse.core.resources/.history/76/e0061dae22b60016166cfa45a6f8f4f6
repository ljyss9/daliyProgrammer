package test1;
import java.util.*;

public class Main{
	 public int thirdMax(int[] nums) {
	        if(nums.length == 1)
	            return nums[0];
	        else if(nums.length == 2)
	            return nums[0] > nums[1] ? nums[0] :nums[1];
	        else{
	            int ano [] = new int[nums.length];
	            int i = 1,j = 1;
	            ano[0] = nums[0];
	            for(i = 1;i < nums.length;i++){
	                if(nums[i] != nums[i-1])
	                    ano[j++] = nums[i];
	            }
	            if(j == 1)
	                return ano[0];
	            else if(j == 2)
	                return ano[0] > nums[1] ? nums[0] :nums[1];
	            else{
	                int max1,max2,max3;
	                if(ano[0] > ano[1]){
	                    if(ano[2] > ano[0]){
	                        max1 = ano[2];
	                        max2 = ano[0];
	                        max3 = ano[3];
	                    }
	                    else if(ano[2] > ano[1]){
	                        max1 = ano[0];
	                        max2 = ano[2];
	                        max3 = ano[1];
	                    }
	                    else{
	                        max1 = ano[0];
	                        max2 = ano[1];
	                        max3 = ano[2];
	                    }
	                }
	                else{
	                    if(ano[2] > ano[1]){
	                        max1 = ano[2];
	                        max2 = ano[1];
	                        max3 = ano[0];
	                    }
	                    else if(ano[2] > ano[0])
	                    {
	                        max1 = ano[1];
	                        max2 = ano[2];
	                        max3 = ano[0];
	                    }
	                    else{
	                        max1 = ano[1];
	                        max2 = ano[0];
	                        max3 = ano[2];
	                    }
	                }
	                for(i = 3;i < j;i++){
	                    if(ano[i] > max1){
	                        max3 = max2;
	                        max2 = max1;
	                        max1 = ano[i];
	                    }
	                    else if(ano[i] > max2){
	                        max3 = max2;
	                        max2 = ano[i];
	                    }
	                    else if(ano[i] > max3){
	                        max3 = ano[i];
	                    }
	                }
	                return max3;
	            }
	             
	        }
	            
	    }
	 
	    public static void main(String args[]){
	    	Main m = new Main();
	    	int []test = {1,2};
	    	System.out.println(m.thirdMax(test)));
	    }
}