package asynCommunication;

import java.awt.*;
import java.awt.event.WindowAdapter;
import java.awt.event.WindowEvent;

public class ClientGraph extends JFrame implements ActionListener{
	private Frame frame;
	private Button bt = new Button("新建客户端线程");
	
	ClientGraph()
	{
		init();
	}
	
	public void init()
	{
		frame = new Frame("Client");
		frame.setBounds(50, 50, 200, 200);
		frame.
		
		frame.add(bt);
		
	}
	
	public static void main(String args[])
	{
		ClientGraph cg = new ClientGraph();
	}
	
}
