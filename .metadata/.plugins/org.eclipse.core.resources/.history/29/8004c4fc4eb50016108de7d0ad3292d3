package test1;
import java.util.*;

public class Main{
	
	 public int arrangeCoins(int n) {
	        if(n == 0 || n == 1)
	            return n;
	        int max = n/2 + 1,min = 1;
	        int mid,res1,res2;
	        while(true){
	            mid = (min + max)/2;
	            res1 = (1+mid)*mid/2;
	            res2 = (1 + mid +1)*(mid+1)/2;
	            if(res1 <= n && res2 > n)
	                return mid;
	            else if(res1 > n){
	                max = mid - 1;
	            }
	            else{
	                min = mid + 1;
	            }
	            
	        }
	    }
	    public static void main(String args[]){
	    	Main m = new Main();
	    	System.out.println(m.originalDigits("fviefuro"));
	    }
}