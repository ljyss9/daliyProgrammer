package test1;
import java.util.*;

public class Main{
	
	 private final String num[] = {"zero","one","two","three","four","five","six","seven","eight","nine"};
	 
	    public String originalDigits(String s) {
	        int chuli[] = new int[26];
	        for(int i = 0;i < s.length();i++){
	            chuli[s.charAt(i) - 'a']++;
	        }
	        StringBuffer st = new StringBuffer();
	        Boolean tt = new Boolean(false);
	        help(chuli,st,tt);
	        return String.valueOf(st);
	    }
	    private boolean judge(int chuli[]){
	        for(int i = 0;i < chuli.length;i++){
	            if(chuli[i] != 0)
	                return false;
	        }
	        return true;
	    }
	    
	    private boolean contain(int chuli[],String s){
	        int temp[] = new int[chuli.length];
	        for(int i = 0;i < chuli.length;i++){
	            temp[i] = chuli[i];
	        }
	        for(int i = 0;i < s.length();i++)
	        {
	            if(chuli[s.charAt(i) - 'a'] <= 0)
	                return false;
	            else{
	                chuli[s.charAt(i) - 'a'] --;
	            }
	        }
	        return true;
	    }
	    
	    public void help(int chuli[],StringBuffer snow, Boolean tt){
	        if(judge(chuli))
	        {
	            tt = true;
	            return;
	        }
	        else{
	            for(int i = 0;i < num.length;i++){
	                if(contain(chuli,num[i])){
	                    for(int j = 0;j < num[i].length();j++)
	                        chuli[num[i].charAt(j) - 'a']--;
	                }
	                snow.append(i);
	                help(chuli,snow,tt);
	                if(tt)
	                    return;
	                else{
	                    snow.delete(snow.length() - 1,snow.length());
	                }
	            }
	        }
	        
	    }
	    public static void main(String args[]){
	    	Main m = new Main();
	    	System.out.println(m.originalDigits("owo"));
	    }
}